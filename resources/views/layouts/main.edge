<script>
    $(function () {
        const Toast = Swal.mixin({toast: true,position: 'top-end',showConfirmButton: false,timer: 4000});
      @if(flashMessage('msgSuccess'))
        Toast.fire({type: 'success',title: '{{{ flashMessage('msgSuccess') }}}'})
      @endif
      @if(flashMessage('msgError'))
        Toast.fire({type: 'warning',title: '{{{ flashMessage('msgError') }}}'})
      @endif
    })
</script>
